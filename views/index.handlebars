    <!--main-->
    <div class="container" id="main">
      <div class="row">
          <!--articles-->
          <div class='col-md-6 col-sm-6'>
            <div class="panel panel-default">
              <div class="panel-heading"><!-- TODO <a href="#" class="pull-right">View all</a> --> <h4>Angular2 Articles</h4></div>
                <div class="panel-body">
                  <ol>
                    {{#each articles}}
                    <li><img width="16px" src="{{img}}" style="margin-right:4px"><a target="new" href="{{url}}">{{name}}</a></li>
                    {{/each}}
                  </ol>
                </div>
              </div>
          </div>
          <!--tutorials-->
          <div class='col-md-6 col-sm-6'>
            <div class="panel panel-default">
              <div class="panel-heading"><!-- TODO <a href="#" class="pull-right">View all</a> --> <h4>Angular2 Tutorials</h4></div>
                <div class="panel-body">
                  <ol>
                    {{#each tutorials}}
                    <li><img width="16px" src="{{img}}" style="margin-right:4px"><a target="new" href="{{url}}">{{name}}</a></li>
                    {{/each}}
                  </ol>
                </div>
              </div>
          </div>
      </div>
      <div class="row">
          <!--ADS-->
          <div class='col-md-6 col-sm-6'>
            <div class="panel panel-default" style="height: 283px;">
              <div class="panel-body" style="text-align: center; overflow: hidden;">
                <iframe width="400" height="250" src="https://www.youtube.com/embed/HmWm21cCAXM" frameborder="0" allowfullscreen></iframe>
              </div>
            </div>
          </div>
          <!--videos-->
          <div class='col-md-6 col-sm-6'>
            <div class="panel panel-default">
              <div class="panel-heading"><!-- TODO <a href="#" class="pull-right">View all</a> --> <h4>Angular2 Videos</h4></div>
                <div class="panel-body">
                  <ol>
                    {{#each videos}}
                    <li><img width="16px" src="{{img}}" style="margin-right:4px"><a target="new" href="{{url}}">{{name}}</a></li>
                    {{/each}}
                  </ol>
                </div>
              </div>
          </div>
      </div>

      <div class="row">
          <!--Featured article 1-->
          <div class='col-md-12 col-sm-12'>
            <div class="panel panel-default">
              <div class="panel-heading"><h4>Featured Article</h4></div>
              <div class="panel-body">
                <p><img src="{{featuredArticle.img}}" class="img-circle pull-left"> <a target="new" href="{{featuredArticle.url}}">{{featuredArticle.name}}</a> ({{featuredArticle.author}})<div class="clearfix"></div><hr>{{featuredArticle.safeContent}}</p>
              </div>
            </div>
          </div>
      </div>

      <div class="row">
          <!--Featured article 2-->
          <div class='col-md-12 col-sm-12'>
            <div class="panel panel-default">
              <div class="panel-heading"><h4>Featured Repository</h4></div>
              <div class="panel-body">
                <p><img src="{{featuredRepo.img}}" class="img-circle pull-left"> <a target="new" href="{{featuredRepo.url}}">{{featuredRepo.name}}</a> ({{featuredRepo.author}})<div class="clearfix"></div><hr>{{featuredRepo.safeContent}}</p>
              </div>
            </div>
          </div>
      </div>
    </div>
